component displayname="SmsProvider" accessors="true" {	property providerId;	property provider;	property email;	property isActive;	/**	* @displayname init	* @description I am the constructor method for SmsProvider	* @returntype  this	*/	public function init( numeric providerId = 0, string provider = '', string email = '', boolean isActive = false ) {		setProviderId( arguments.providerId );		setProvider( arguments.provider );		setEmail( arguments.email );		setIsActive( arguments.isActive );		return this;	}	// SETTERS //	public function setProviderId( numeric providerId = 0 ) {		variables.providerId = arguments.providerId;	}	public function setProvider( string provider = '' ) {		variables.provider = arguments.provider;	}	public function setEmail( string email = '' ) {		variables.email = arguments.email;	}	public function setIsActive( boolean isActive = false ) {		variables.isActive = arguments.isActive;	}	// GETTERS //	public function getProviderId() {		return variables.providerId;	}	public function getProvider() {		return variables.provider;	}	public function getEmail() {		return variables.email;	}	public function getIsActive() {		return variables.isActive;	}	// UTILITY //	public function getMemento() {		return variables;	}}