/**** @file Session.cfc* @author Denard Springle ( denard.springle@gmail.com )* @description I am a session bean used for storing user sessions**/component displayname="Session" accessors="true" {	property userId;	property sessionId;	property firstName;	property lastName;	property role;	property mfaCode;	property isAuthenticated;	property lastActionAt;	/**	* @displayname init	* @description I am the constructor method for Session	* @return Type  this	*/	public function init( numeric userId = 0, string sessionId = '', string firstName = '', string lastName = '', numeric role = 0, string mfaCode = '', boolean isAuthenticated = false, date lastActionAt = '2010-01-01 00:00:00' ) {		setUserId( arguments.userId );		setSessionId( arguments.sessionId );		setFirstName( arguments.firstName );		setLastName( arguments.lastName );		setRole( arguments.role );		setMfaCode( arguments.mfaCode );		setIsAuthenticated( arguments.isAuthenticated );		setLastActionAt( arguments.lastActionAt );		return this;	}	// SETTERS //	public function setUserId( numeric userId = 0 ) {		variables.userId = arguments.userId;	}	public function setSessionId( string sessionId = '' ) {		variables.sessionId = arguments.sessionId;	}	public function setFirstName( string firstName = '' ) {		variables.firstName = arguments.firstName;	}	public function setLastName( string lastName = '' ) {		variables.lastName = arguments.lastName;	}	public function setRole( numeric role = 0 ) {		variables.role = arguments.role;	}	public function setMfaCode( string mfacode = '' ) {		variables.mfaCode = arguments.mfaCOde;	}	public function setIsAuthenticated( boolean isAuthenticated = false ) {		variables.isAuthenticated = arguments.isAuthenticated;	}	public function setLastActionAt( date lastActionAt = '2010-01-01 00:00:00' ) {		variables.lastActionAt = arguments.lastActionAt;	}	// GETTERS //	public function getUserId() {		return variables.userId;	}	public function getSessionId() {		return variables.sessionId;	}	public function getFirstName() {		return variables.firstName;	}	public function getLastName() {		return variables.lastName;	}	public function getRole() {		return variables.role;	}	public function getMfaCode() {		return variables.mfaCode;	}	public function getIsAuthenticated() {		return variables.isAUthenticated;	}	public function getLastActionAt() {		return variables.lastActionAt;	}	// UTILITY //	public function getMemento() {		return variables;	}}